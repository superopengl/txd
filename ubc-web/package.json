{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@craco/craco": "^5.6.4",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "antd": "^4.2.4",
    "axios": "^0.19.2",
    "craco-less": "^1.16.0",
    "lodash": "^4.17.15",
    "moment": "^2.25.3",
    "moment-random": "^1.0.5",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-anchor-link-smooth-scroll": "^1.0.12",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1",
    "styled-components": "^5.1.0"
  },
  "scripts": {
    "start": "cross-env PORT=5006 craco start",
    "build": "cross-env GENERATE_SOURCEMAP=false craco build",
    "build:prod": "cross-env GENERATE_SOURCEMAP=false craco build",
    "test": "craco test",
    "eject": "react-scripts eject",
    "dev": "yarn && yarn build && yarn start",
    "watch": "npm-watch",
    "g": "npx generate-react-cli component --",
    "p": "npx generate-react-cli component -- --type=page"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "babel-plugin-import": "^1.13.0",
    "cross-env": "^7.0.2",
    "generate-react-cli": "^5.0.0",
    "less-loader": "^6.1.0",
    "npm-watch": "^0.6.0"
  },
  "watch": {
    "build": {
      "patterns": [
        "src"
      ],
      "extensions": "js,jsx"
    }
  }
}
