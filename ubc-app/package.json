{
  "name": "ubc-app",
  "version": "1.0.0",
  "description": "UBC server",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm run build && cd build && cross-env UBC_PORT=5005 node index.js",
    "dev": "cross-env UBC_PORT=5005 ts-node index.ts",
    "build": "npm run lint && npm run clean && npm run compile && npm run copy",
    "clean": "rm -rf build",
    "compile": "tsc",
    "lint": "tslint -c tslint.json -p tsconfig.json --fix",
    "copy": "cp -rf keys build/keys && cp -rf ../ubc-web/build build/www && cp -f ormconfig.json build/ormcofig.json"
  },
  "author": "ubc.techseeding@gmail.com",
  "license": "MIT",
  "devDependencies": {
    "cross-env": "^7.0.2",
    "ts-node": "^8.10.1",
    "tslint": "^6.1.2",
    "typescript": "^3.9.3"
  },
  "dependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/express": "^4.17.6",
    "@types/node": "^14.0.5",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-fileupload": "^1.1.7-alpha.3",
    "express-list-endpoints": "^4.0.1",
    "http-assert": "^1.4.1",
    "lodash": "^4.17.15",
    "moment": "^2.26.0",
    "pg": "^8.2.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.25",
    "uuid": "^8.1.0"
  }
}
